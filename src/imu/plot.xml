<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="1" columns="3" carrier="fast_tcp">
      <plot gridx="0"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Roll"
            minval="-180"
            maxval="180"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                        index="0"
                        color="Red"
                        title="Graph 0"
                        type="lines"
                        size="2" />
                  <graph remote="/test-imu/out"
                        index="3"
                        color="Blue"
                        title="Graph 0"
                        type="lines"
                        size="2" />  
      </plot>
      <plot gridx="1"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Pitch"
            minval="-180"
            maxval="180"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                        index="1"
                        color="Red"
                        title="Graph 1"
                        size="2"
                        type="lines" />
                  <graph remote="/test-imu/out"
                        index="4"
                        color="Blue"
                        title="Graph 1"
                        size="2"
                        type="lines" /> 
      </plot>
      <plot gridx="2"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Yaw"
            minval="-180"
            maxval="180"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                        index="2"
                        color="Red"
                        title="Graph 2"
                        size="2"
                        type="lines" />
                  <graph remote="/test-imu/out"
                        index="5"
                        color="Blue"
                        title="Graph 2"
                        size="2"
                        type="lines" />  
      </plot>
</portscope>
