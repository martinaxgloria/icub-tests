<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="2" columns="2" carrier="fast_tcp">
      <plot gridx="0"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Roll"
            minval="-5"
            maxval="5"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                        index="0"
                        color="Red"
                        title="Graph 0"
                        type="lines"
                        size="2" />
      </plot>
      <plot gridx="0"
            gridy="1"
            hspan="1"
            vspan="1"
            title="Pitch"
            minval="-5"
            maxval="5"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                  index="1"
                  color="Green"
                  title="Graph 1"
                  size="2"
                  type="lines" />
      </plot>
      <plot gridx="1"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Yaw"
            minval="-5"
            maxval="5"
            bgcolor="LightSlateGrey">
                  <graph remote="/test-imu/out"
                  index="2"
                  color="Blue"
                  title="Graph 2"
                  size="2"
                  type="lines" />
      </plot>
</portscope>
